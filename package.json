    "@inrupt/solid-client-vc": "^1.2.0",
    "jose": "^5.9.6",
    "loglevel": "^1.9.2",
    "n3": "^1.23.1",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/n3": "^1.21.1",
    "@types/node": "^22.10.6",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.7",
    "rollup": "^4.30.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.7.3"
