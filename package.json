{
  "name": "@weare/weare-core",
  "version": "0.0.28",
  "description": "A We Are library that contains helper methods that can be used when developing We Are apps.",
  "author": "VITO NV",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup --config rollup.config.mjs"
  },
  "main": "dist/main.js",
  "module": "dist/main.mjs",
